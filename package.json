{
  "name": "my-app",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "engines": {
    "node": "8.9.1"
  },
  "scripts": {
    "start": "node dist/src/server.js",
    "test": "mocha \"dist/test/**/*.js\"",
    "build": "npm run clean && npm run build-ts && npm run copy-static-assets",
    "clean": "rimraf dist/*",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-sass\" \"npm run watch-ts\" \"npm run serve-debug\"",
    "serve-debug": "nodemon --inspect dist/src/server.js",
    "heroku:connect": "heroku git:remote -a reviewapi-dev",
    "deploy": "npm run build && heroku container:push web",
    "copy-static-assets": "node copyStaticAssets.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "dotenv": "4.0.0",
    "express": "^4.15.5",
    "express-validator": "5.0.3",
    "mongodb": "^2.2.31",
    "request": "^2.83.0",
    "simple-oauth2": "^1.4.0",
    "swagger-ui-express": "^2.0.8",
    "winston": "^2.3.1",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.5",
    "@types/chai": "^4.0.4",
    "@types/cors": "^2.8.1",
    "@types/express": "^4.0.37",
    "@types/express-validator": "^3.0.0",
    "@types/mocha": "^2.2.43",
    "@types/mongodb": "^2.2.11",
    "@types/request": "^2.0.8",
    "@types/simple-oauth2": "^1.0.4",
    "@types/supertest": "^2.0.3",
    "@types/swagger-jsdoc": "0.0.1",
    "@types/winston": "^2.3.6",
    "chai": "^4.1.2",
    "mocha": "^3.5.3",
    "rimraf": "2.6.2",
    "shelljs": "^0.7.8",
    "supertest": "^3.0.0",
    "tslint": "^5.7.0",
    "tslint-microsoft-contrib": "^5.0.1",
    "typescript": "^2.5.2",
    "typescript-eslint-parser": "^8.0.0"
  }
}
